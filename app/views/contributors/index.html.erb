<div class="container">

  <p id="notice"><%= notice %></p>

  <h2>Colaboradores</h2>
  <br>
  <table>
    <thead>
      <tr>
        <th>Nombre</th>
        <th>Email</th>
        <th>Telefono</th>
        <th>Caracteristicas y habilidades</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @contributors.each do |contributor| %>
        <tr>
          <td><%= contributor.name %></td>
          <td><%= contributor.email %></td>
          <td><%= contributor.phone %></td>
          <td><%= contributor.skills %></td>
          <td><a class="waves-effect waves-light btn"<%= link_to 'Mostrar', contributor %></td></a>

          <td><a class="waves-effect waves-light btn"<%= link_to 'Editar', edit_contributor_path(contributor) %></td></a>

          <td><a class="waves-effect waves-light btn"<%= link_to 'Eliminar', contributor, method: :delete, data: { confirm: 'Estas seguro que deseas eliminar?' } %></td></a>

        </tr>
      <% end %>
    </tbody>
  </table>

  <br>
<a class="waves-effect waves-light btn"  <%= link_to 'Nuevo colaborador', new_contributor_path %></a>
<br>
<br>

</div>
