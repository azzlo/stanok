<div class="container">
  <%= form_for(contributor) do |f| %>
    <% if contributor.errors.any? %>
      <div id="error_explanation">
        <h2><%= pluralize(contributor.errors.count, "error") %> prohibited this contributor from being saved:</h2>

        <ul>
        <% contributor.errors.full_messages.each do |message| %>
          <li><%= message %></li>
        <% end %>
        </ul>
      </div>
    <% end %>
    <div class="row">
      <div class="field input-field col s12 m10 l8">
        <i class="material-icons prefix">mode_edit</i>
        <%= f.label :name %>
        <%= f.text_field :name %>
      </div>
    </div>
    <div class="row">
      <div class="field input-field col s12 m6 l6">
        <i class="material-icons prefix">email</i>
        <%= f.label :email %>
        <%= f.text_field :email %>
      </div>

      <div class="field input-field col s12 m6 l5">
        <i class="material-icons prefix">phone</i>
        <%= f.label :phone %>
        <%= f.number_field :phone %>
      </div>
    </div>
    <div class="row">

          <div class="field input-field col s12 m12 l10">
            <i class="material-icons prefix">person_pin</i>
            <%= f.label :skills %>
            <%= f.text_field :skills %>
          </div>
    </div>

    <br>

    <div class="actions waves-effect waves-light btn lime darken-3">
      <%= f.submit %>
    </div>
  <% end %>


</div>
